    "pub": "git push --follow-tags origin master && npm publish",
    "test": "jest"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@types/es6-promise": "^3.3.0",
    "@types/node": "^10.12.14",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.6.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "standard-version": "^4.4.0",
    "ts-loader": "^5.3.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "request",
    "AOP"
  ]
