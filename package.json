{
  "name": "request-provider",
  "version": "0.0.8",
  "description": "request library with AOP",
  "main": "./dist/index.js",
  "repository": "https://github.com/Lighting-Jack/request-provider",
  "author": "Lighting-Jack",
  "license": "MIT",
  "scripts": {
    "init": "commitizen init cz-conventional-changelog --save --save-exact",
